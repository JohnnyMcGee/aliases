[alias]
	aa = !git add --all && git status
	aliases = config --get-regexp alias
	br = branch
	ca = commit --amend
	car = commit --amend --no-edit
	cm = !git commit && git log --oneline --decorate --graph --all -10
	cma = !git commit -a && git log --oneline --decorate --graph --all -10
	co = checkout
	dc = diff --word-diff --cached --color-words
	df = diff --word-diff --color-words
	f = fetch
	p = push
	psuo = push --set-upstream origin
	pdel = push origin --delete
	pl = !git pull && git log --oneline --decorate --graph --all -10
	rs = reset
	sl = log --oneline --decorate --graph -20
	sla = log --oneline --decorate --graph --all -20
	slam = log --oneline --decorate --graph --all
	mpm = !git checkout main && git merge - && git push
	rb = rebase
	rbi = rebase -i
	rbc = !git add --all && git rebase --continue
	rao = remote add origin
	conn = !git branch -m main && git push -u origin main
