[alias]
	aa = !git add --all && git status
	aliases = config --get-regexp alias
	br = branch
	ca = commit --amend
	caa = commit -a --amend -C HEAD
	car = commit --amend --no-edit
	cm = !git commit && git log --oneline --decorate --graph --all -10
	cma = !git commit -a && git log --oneline --decorate --graph --all -10
	cmm = commit -a -m
	cap = !git commit -a --amend -C HEAD && git push
	capf = !git commit -a --amend -C HEAD && git push -f
	co = checkout
	dc = diff --word-diff --cached --color-words
	df = diff --word-diff --color-words
	f = !git fetch && git log --oneline --decorate --graph --all -10
	p = push
	psuo = push --set-upstream origin
	psuoc = !git push --set-upstream origin $(git branch --show-current)
	pdel = push origin --delete
	pl = !git pull && git log --oneline --decorate --graph --all -10
	plm = !git checkout main && git pull && git log --oneline --decorate --graph --all -10
	rs = reset
	sl = log --oneline --decorate --graph -20
	sla = log --oneline --decorate --graph --all -20
	slam = log --oneline --decorate --graph --all
	mpm = !git checkout main && git merge - && git push
	rb = rebase
	rbi = rebase -i
	rbc = !git add --all && git rebase --continue
	rao = remote add origin
	conn = !git branch -m main && git push -u origin main
	nb = "!f () { git checkout -b jbm/$1; }; f"
	hf = "!f () { git checkout -b hf-$1; }; f"
	wipe = reset --hard HEAD
	rmc = rm --cached
	s = status
	cb = branch --show-current
	st = subtree
	sm = submodule
	main = "!git checkout main && git pull"
	dev = "!git checkout dev && git pull"
    brf = "!git branch | fzf --cycle | xargs -I {} git checkout {}"
